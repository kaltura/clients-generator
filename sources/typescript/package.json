{
  "name": "kaltura-typescript-client",
  "private": true,
  "version": "@VERSION@",
  "description": "Kaltura Typescript client",
  "keywords": [
    "Kaltura"
  ],
  "license": "AGPL-3.0",
  "author": "https://github.com/kaltura-ng/graphs/contributors",
  "contributors": [
    {
      "name": "Eran Sakal",
      "email": "eran@sakalim.com"
    },
    {
      "name": "Amir Chervinsky",
      "email": "amir.chr@gmail.com"
    }
  ],
  "scripts": {
    "build": "gulp build",
    "build:prod": "gulp build --production",
    "lint": "tslint src/**/*.ts",
    "watch": "gulp watch",
    "deploy": "rm -rf dist && npm run build && cd dist && npm pack && npm link",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kaltura/KalturaGeneratedAPIClientsTypescript.git"
  },
  "bugs": {
    "url": "https://github.com/kaltura/KalturaGeneratedAPIClientsTypescript/issues"
  },
  "main": "./index.js",
  "dependencies": {
    "@types/node": "^10.3.3",
    "tslib": "^1.5.0"
  },
  "devDependencies": {
    "@types/core-js": "^2.5.0",
    "@types/jest": "^23.1.0",
    "@types/jquery": "^3.3.2",
    "@types/ramda": "0.25.33",
    "awesome-typescript-loader": "~5.0.0",
    "core-js": "2.5.7",
    "del": "^3.0.0",
    "gulp": "^3.9.1",
    "gulp-add-src": "^1.0.0",
    "gulp-json-editor": "^2.2.1",
    "gulp-load-plugins": "^1.4.0",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-tslint": "~8.1.3",
    "gulp-typescript": "^5.0.0-alpha.1",
    "gulp-util": "^3.0.7",
    "jest": "^23.1.0",
    "merge2": "^1.0.2",
    "rimraf": "^2.5.1",
    "run-sequence": "2.2.1",
    "standard-version": "^4.0.0",
    "ts-jest": "^22.4.6",
    "tslint": "5.10.0",
    "tslint-loader": "3.6.0",
    "typescript": "^2.4.0",
    "yargs": "^11.0.0"
  },
  "engines": {
    "node": ">= 7.0.0",
    "npm": ">= 5.0.0"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testMatch": ["**/tests/*.spec.ts"],
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ]
  }
}
