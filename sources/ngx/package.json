{
  "name": "kaltura-ngx-client",
  "private": true,
  "version": "10.1.0",
  "description": "Kaltura Angular based client",
  "keywords": [
    "Kaltura"
  ],
  "license": "AGPL-3.0",
  "author": "https://github.com/kaltura-ng/graphs/contributors",
  "contributors": [
    {
      "name": "Eran Sakal",
      "email": "eran@sakalim.com"
    },
    {
      "name": "Amir Chervinsky",
      "email": "amir.chr@gmail.com"
    }
  ],
  "scripts": {
    "build": "rm -rf dist && gulp build:esm",
    "lint": "tslint src/**/*.ts",
    "watch": "gulp watch",
    "deploy": "rm -rf dist && npm run build && cd dist && npm pack && npm link",
    "test": "jest"
  },
  "config": {
    "publishDirectory": "dist"
  },
  "main": "./index.js",
  "dependencies": {
    "@angular/core": "^5.2.0",
    "@angular/common": "^5.2.0"
  },
  "devDependencies": {
    "@angular/compiler": "^5.2.0",
    "@angular/compiler-cli": "^5.2.0",
    "@angular/platform-browser": "^5.2.0",
    "@angular/platform-browser-dynamic": "^5.2.0",
    "@types/core-js": "^0.9.37",
    "@types/jest": "^21.1.8",
    "@types/jquery": "^2.0.40",
    "@types/node": "^8.0.47",
    "core-js": "2.4.1",
    "del": "^2.2.2",
    "gulp": "^3.9.1",
    "gulp-load-plugins": "^1.4.0",
    "jest": "^21.2.1",
    "jest-preset-angular": "^4.0.2",
    "merge2": "^1.0.2",
    "reflect-metadata": "^0.1.10",
    "rimraf": "^2.5.1",
    "run-sequence": "1.2.2",
    "rxjs": "^5.5.6",
    "standard-version": "^4.0.0",
    "through2": "^2.0.3",
    "ts-jest": "^21.1.3",
    "tslib": "^1.5.0",
    "tslint": "4.3.1",
    "tslint-loader": "3.2.1",
    "typescript": "^2.4.2",
    "zone.js": "^0.8.18"
  },
  "engines": {
    "node": ">= 7.0.0",
    "npm": ">= 5.0.0"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupTestFrameworkScriptFile": "<rootDir>/src/tests/setup-jest.ts",
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  }
}
